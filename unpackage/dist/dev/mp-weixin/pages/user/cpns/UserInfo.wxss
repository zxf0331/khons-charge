@font-face {
  font-family: "iconfont"; /* Project id 4462692 */
  src: url('data:font/woff2;base64,') format('woff2'),
       url('data:font/woff;base64,') format('woff'),
       url('data:font/ttf;base64,') format('truetype');
}
.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.icon-right-arrow:before {
  content: "\e617";
}
.icon-set:before {
  content: "\e61c";
}
 .uni-card__header-content-title span {
  font-weight: 600;
}
.userinfo {
  background-image: linear-gradient(to bottom, #e0ecfc, #fcfdff);
}
.userinfo .info-top {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding: 20rpx;
}
.userinfo .info-top .info {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.userinfo .info-top .info .avatar {
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  margin-right: 20rpx;
}
.userinfo .info-top .info .userdetail {
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}
.userinfo .info-top .info .userdetail text {
  margin: 4rpx 0;
}
.userinfo .info-top .info .userdetail .username {
  font-size: 36rpx;
  font-weight: 600;
}
.userinfo .info-top .set {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  position: absolute;
  right: 20rpx;
  top: 20rpx;
}
.userinfo .info-bottom {
  margin-top: 16rpx;
  padding-bottom: 30rpx;
}
.userinfo .info-bottom .wallet {
  display: flex;
  flex-direction: row;
  justify-content: space-around;
}
.userinfo .info-bottom .wallet .wallet-item .item-num {
  text-align: center;
  font-size: 40rpx;
  font-weight: 600;
}
.userinfo .info-bottom .wallet .wallet-item .item-name {
  margin-top: 4rpx;
  font-size: 28rpx;
  color: #666666;
}
.userinfo .info-bottom .wallet .wallet-item .item-name .icon-right-arrow {
  display: inline;
  font-size: 28rpx;
}